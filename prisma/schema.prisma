// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  //  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model RadCheck {
  id        Int    @id @default(autoincrement())
  username  String
  attribute String
  op        String
  value     String

  @@map("radcheck")
}

model RadPostAuth {
  id       Int      @id @default(autoincrement())
  username String
  pass     String
  reply    String
  authDate DateTime

  @@map("radpostauth")
}

model RadReply {
  id        Int    @id @default(autoincrement())
  username  String
  attribute String
  op        String
  value     String

  @@map("radreply")
}
